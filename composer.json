{
    "name": "gemingcao/slim3-application",
    "description": "A Slim Framework application for rapid development",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/gemingcao/slim3-application",
    "license": "MIT",
    "authors": [
        {
            "name": "gemingcao",
            "email": "gemingcao@qq.com",
            "homepage": "http://www.gemingcao.com/"
        }
    ],
    "support": {
        "source": "https://github.com/gemingcao/slim3-application.git"
    },
   "autoload": {
        "psr-4": {
            "App\\": "./application/"
        }
    },
    "require": {
        "php": ">=7.1",
        "slim/slim": "^3.1",
        "slim/twig-view": "^2.4.0",
        "twig/twig": "^1.35.4",
        "slim/csrf": "^0.8.3",
        "monolog/monolog": "^1.22",
        "orx0r/slim3-controller":"^0.1.2",
        "bryanjhv/slim-session": "^3.6.1",
        "illuminate/database":"^5.7.5",
        "sybio/image-workshop": "^2.0",
        "symfony/dom-crawler":"^3.4.15",
        "gemingcao/cookie-helper":"^1.0",
        "overtrue/wechat": "^4.1"
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public"
    }
